{
  "name": "themoviedb",
  "version": "0.0.1",
  "description": "themoviedb API client",
  "license": "MIT",
  "homepage": "http://github.com/amilajack/themoviedb",
  "repository": "git://github.com/amilajack/themoviedb.git",
  "author": "Amila Welihinda <amiajack@gmail.com>",
  "main": "index.js",
  "browser": {
    "index.js": "./dist/api.js"
  },
  "keywords": [
    "themoviedb",
    "client",
    "api",
    "json"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production babel src --out-dir dist && npm run build-web",
    "build-web": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.js --progress --profile --colors --display-error-details",
    "clean": "rm -rf dist",
    "flow-typed": "flow-typed install",
    "lint": "cross-env NODE_ENV=test eslint --cache --ignore-path .gitignore --format=node_modules/eslint-formatter-pretty src test",
    "precommit": "npm test",
    "spec": "cross-env NODE_ENV=test mocha --timeout 30000 --inline-diffs --async-only --growl --compilers js:babel-register --recursive --require ./test/setup.js test/*.spec.js",
    "test": "cross-env NODE_ENV=test npm-check --production && npm run lint && npm run spec && npm run build",
    "version": "npm run build"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-add-module-exports": "^0.3.3",
    "babel-plugin-transform-es2015-modules-umd": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "cross-env": "^3.1.3",
    "eslint": "^3.9.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-plugin-flowtype": "^2.25.0",
    "eslint-plugin-flowtype-errors": "^1.5.0",
    "eslint-plugin-fp": "^2.2.0",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-react": "^6.4.1",
    "flow-bin": "^0.34.0",
    "flow-typed": "^2.0.0",
    "husky": "^0.11.9",
    "json-loader": "^0.5.4",
    "mocha": "^2.1.2",
    "npm-check": "^5.4.0",
    "webpack": "^1.13.3"
  },
  "dependencies": {
    "babel-runtime": "^6.18.0",
    "isomorphic-fetch": "^2.2.1"
  },
  "engines": {
    "node": ">=4.x",
    "npm": ">=3.x"
  },
  "devEngines": {
    "node": ">=4.x",
    "npm": ">=3.x"
  }
}
