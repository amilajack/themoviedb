{
  "name": "@amilajack/themoviedb",
  "version": "0.0.2",
  "description": "themoviedb API client",
  "license": "MIT",
  "homepage": "http://github.com/amilajack/themoviedb",
  "repository": "git://github.com/amilajack/themoviedb.git",
  "author": "Amila Welihinda <amiajack@gmail.com>",
  "main": "./dist",
  "files": [
    "dist"
  ],
  "keywords": [
    "moviedb",
    "themoviedb",
    "client",
    "promise",
    "api",
    "json"
  ],
  "scripts": {
    "build": "rm -rf dist && cross-env NODE_ENV=production babel src --out-dir dist",
    "flow-typed": "flow-typed install",
    "lint": " eslint . --ignore-path .gitignore",
    "precommit": "yarn test",
    "spec": "jest .",
    "test": "cross-env NODE_ENV=test yarn lint && yarn spec && yarn build",
    "version": "yarn build"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-flow": "^7.9.0",
    "@babel/register": "^7.9.0",
    "babel-core": "^7.0.0-bridge",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.5.1",
    "chai": "^4.2.0",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-bliss": "^4.6.0",
    "eslint-plugin-flowtype-errors": "^4.2.0",
    "flow-bin": "^0.123.0",
    "flow-typed": "^2.6.2",
    "husky": "^3.1.0",
    "jest": "^27.5.1",
    "regenerator-runtime": "^0.13.5"
  },
  "dependencies": {
    "isomorphic-fetch": "^2.2.1"
  },
  "engines": {
    "node": ">=10.x",
    "npm": ">=6.14.5"
  },
  "babel": {
    "presets": [
      ["@babel/preset-env", {
        "targets": {
          "node": 10
        }
      }],
      "@babel/preset-flow"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "eslintConfig": {
    "extends": "bliss",
    "rules": {
      "flowtype-errors/show-errors": "warn"
    }
  },
  "renovate": {
    "extends": [
      "bliss"
    ]
  }
}
